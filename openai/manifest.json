{
  "manifest_version": 3,
  "name": "OpenAI Quick Access",
  "version": "1.0",
  "description": "Quick access to OpenAI(GPT) from address bar with 'o'",
  "permissions": [
    "activeTab",
    "scripting"
  ],
  "host_permissions": [
    "https://chatgpt.com/*"
  ],
  "omnibox": {
    "keyword": "o"
  },
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "16": "icons/icon_16x16.png",
    "48": "icons/icon_48x48.png",
    "128": "icons/icon_128x128.png"
  }
}
